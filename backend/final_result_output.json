  "sql_query": "SELECT\n    DATE_FORMAT(i.InvoiceDate, '%Y-%m') AS SaleMonth,\n    COUNT(DISTINCT il.InvoiceLineId) AS AlbumsSold\nFROM InvoiceLine il\nJOIN Track t ON il.TrackId = t.TrackId\nJOIN Album a ON t.AlbumId = a.AlbumId\nJOIN Invoice i ON il.InvoiceId = i.InvoiceId\nGROUP BY SaleMonth\nORDER BY SaleMonth;",
  "sql_dialect": "MYSQL",
